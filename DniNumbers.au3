#Region Convert to Base 25 -> Output Array

	Func convert(ByRef $input)
		Dim $c = 0
		Dim $x = 2
		Dim $div
		Dim $out[20][20]
		While 1
			While $x >= 1
				$c += 1
				$div = 25 ^ $c
				$x = $input / $div
			WEnd
			$c -= 1
			$out[$co][0] = Int($input / (25 ^ $c))
			$out[$co][1] = 25 ^ $c
			$co += 1
			$input -= Int($input / (25 ^ $c)) * Int(25 ^ $c)
			If $c = 0 Then ExitLoop
		WEnd
		Return $out
	EndFunc

